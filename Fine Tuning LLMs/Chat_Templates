   "cell_type": "code",
   "source": [
    "from transformers import AutoTokenizer\n",
    "\n",
    "CHATML_TEMPLATE = \"\"\"{% for m in messages %}\n",
    "{% if m['role'] == 'system' %}{{ '<|im_start|>system\\n' + m['content'] + '<|im_end|>\\n' }}\n",
    "{% elif m['role'] == 'user' %}{{ '<|im_start|>user\\n' + m['content'] + '<|im_end|>\\n' }}\n",
    "{% elif m['role'] == 'assistant' %}{{ '<|im_start|>assistant\\n' + m['content'] + '<|im_end|>\\n' }}\n",
    "{% endif %}{% endfor %}{% if add_generation_prompt %}{{ '<|im_start|>assistant\\n' }}{% endif %}\"\"\"\n",
    "\n",
    "def ensure_template(tok):\n",
    "    if not getattr(tok, \"chat_template\", None):\n",
    "        tok.chat_template = CHATML_TEMPLATE\n",
    "    return tok\n",
    "\n",
    "qwen_tok = ensure_template(AutoTokenizer.from_pretrained(\"Qwen/Qwen-7B-Chat\", trust_remote_code=True))\n",
    "smol_tok = ensure_template(AutoTokenizer.from_pretrained(\"HuggingFaceTB/SmolLM2-135M-Instruct\"))\n",
    "\n",
    "messages = [\n",
    "    {\"role\": \"system\", \"content\": \"You are a helpful assistant.\"},\n",
    "    {\"role\": \"user\", \"content\": \"Hello!\"},\n",
    "]\n",
    "\n",
    "qwen_chat = qwen_tok.apply_chat_template(messages, tokenize=False, add_generation_prompt=True)\n",
    "smol_chat = smol_tok.apply_chat_template(messages, tokenize=False, add_generation_prompt=True)\n",
    "print(qwen_chat[:400])\n",
    "print(smol_chat[:400])\n"
   ],
   "metadata": {
    "colab": {
     "base_uri": "https://localhost:8080/"
    },
    "id": "6UkvzFnELCDh",
    "outputId": "f20897bd-266d-4b78-a1e2-61eaf227e8cd"
   },
   "execution_count": 14,
   "outputs": [
    {
     "output_type": "stream",
     "name": "stdout",
     "text": [
      "<|im_start|>system\n",
      "You are a helpful assistant.<|im_end|>\n",
      "\n",
      "<|im_start|>user\n",
      "Hello!<|im_end|>\n",
      "\n",
      "<|im_start|>assistant\n",
      "\n",
      "<|im_start|>system\n",
      "You are a helpful assistant.<|im_end|>\n",
      "<|im_start|>user\n",
      "Hello!<|im_end|>\n",
      "<|im_start|>assistant\n",
      "\n"
     ]
    }
